    <servlet-mapping>
        <servlet-name>InitializeDownloadServet</servlet-name>
        <url-pattern>/InitializeDownloadServet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadFileServlet</servlet-name>
        <url-pattern>/ReadFileServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
